{
    "name": "ept/laravel-async",
    "description": "Laravel Async",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Saeed Vaziry",
            "email": "mr.saeedvaziry@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "php": "^7.4|^8.1",
        "illuminate/support": "~8.0",
        "illuminate/process": "~8.0",
        "illuminate/console": "~8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "SaeedVaziry\\LaravelAsync\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SaeedVaziry\\LaravelAsync\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SaeedVaziry\\LaravelAsync\\LaravelAsyncServiceProvider"
            ],
            "aliases": {
                "AsyncHandler": "SaeedVaziry\\LaravelAsync\\Facades\\AsyncHandler"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
